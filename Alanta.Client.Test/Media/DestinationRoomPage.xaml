<UserControl 
    x:Class="Alanta.Client.Test.Media.DestinationRoomPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:roomView="clr-namespace:Alanta.Client.UI.Desktop.RoomView;assembly=Alanta.Client.UI.Desktop"
    xmlns:av="clr-namespace:Alanta.Client.UI.Desktop.Controls.AudioVisualizer;assembly=Alanta.Client.UI.Desktop"
    mc:Ignorable="d"
    d:DesignHeight="200" d:DesignWidth="800">
    <Border BorderBrush="Gray" BorderThickness="1" >
        <StackPanel x:Name="LayoutRoot" Background="White" Height="200" Orientation="Horizontal">
            <av:AudioVisualizer x:Name="audioVisualizer" Grid.Column="1" Height="200" Width="300"/>
        </StackPanel>
    </Border>
</UserControl>
